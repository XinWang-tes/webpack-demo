#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wangxin/mycode/webpack-demo/node_modules/.pnpm/webpack-dev-server@4.9.0_webpack-cli@4.9.2+webpack@5.72.0/node_modules/webpack-dev-server/bin/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/.pnpm/webpack-dev-server@4.9.0_webpack-cli@4.9.2+webpack@5.72.0/node_modules/webpack-dev-server/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/.pnpm/webpack-dev-server@4.9.0_webpack-cli@4.9.2+webpack@5.72.0/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/.pnpm/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules:/Users/wangxin/mycode/node_modules:/Users/wangxin/node_modules:/Users/node_modules:/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/webpack-dev-server/bin/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/webpack-dev-server/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/wangxin/mycode/webpack-demo/node_modules/.pnpm/webpack-dev-server@4.9.0_webpack-cli@4.9.2+webpack@5.72.0/node_modules/webpack-dev-server/bin/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/.pnpm/webpack-dev-server@4.9.0_webpack-cli@4.9.2+webpack@5.72.0/node_modules/webpack-dev-server/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/.pnpm/webpack-dev-server@4.9.0_webpack-cli@4.9.2+webpack@5.72.0/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/.pnpm/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules:/Users/wangxin/mycode/node_modules:/Users/wangxin/node_modules:/Users/node_modules:/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/webpack-dev-server/bin/node_modules:/Users/wangxin/mycode/webpack-demo/node_modules/webpack-dev-server/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
